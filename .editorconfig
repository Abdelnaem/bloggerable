# editorconfig.org

root = true

[*]
charset = utf-8
end_of_line = lf
indent_size = 2
indent_style = space
insert_final_newline = true
trim_trailing_whitespace = true

[*.md]
trim_trailing_whitespace = false

[*.py]
indent_size = 4

[src/**.xml]
insert_final_newline = false

[src/**.css]
insert_final_newline = false

[src/**.js]
insert_final_newline = false
