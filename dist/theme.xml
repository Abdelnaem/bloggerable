  content: "";
}

/* Grid */
body#layout .layout-mode-col-1,
body#layout .layout-mode-col-2,
body#layout .layout-mode-col-3,
body#layout .layout-mode-col-4,
body#layout .layout-mode-col-5,
body#layout .layout-mode-col-6,
body#layout .layout-mode-col-7,
body#layout .layout-mode-col-8,
body#layout .layout-mode-col-9,
