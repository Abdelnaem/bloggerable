<?xml version='1.0' encoding='UTF-8' ?>
<!DOCTYPE html>
<html b:css='false' b:defaultwidgetversion='2' b:layoutsVersion='3' expr:dir='data:blog.languageDirection' expr:lang='data:blog.locale' xmlns='http://www.w3.org/1999/xhtml' xmlns:b='http://www.google.com/2005/gml/b' xmlns:data='http://www.google.com/2005/gml/data' xmlns:expr='http://www.google.com/2005/gml/expr'>


<b:comment>
#############################################################################
Settings
############################################################################# </b:comment>

<b:with value='"https://example.com/1200x630.png"' var='social-logo'>
<b:with value='"replace me"' var='facebook-app-id'>
<b:with value='"@replace_me"' var='twitter-username-website'>
<b:with value='"@replace_me"' var='twitter-username-creator'>


<head>


<b:comment>
#############################################################################
Title, meta and link tags
############################################################################# </b:comment>

<b:comment>### Required meta tags ###</b:comment>

<meta charset='utf-8'/>
<meta name='viewport' content='width=device-width, initial-scale=1, shrink-to-fit=no'/>

<b:comment>### Title ###</b:comment>

<b:comment>=== Homepage ===</b:comment>
<b:if cond='data:view.isHomepage'>
  <title><data:blog.title/></title>
</b:if>
<b:comment>=== Item page ===</b:comment>
<b:if cond='data:view.isPost'>
  <title><data:blog.pageName/> | <data:blog.title/></title>
</b:if>
<b:comment>=== Static page ===</b:comment>
<b:if cond='data:view.isPage'>
  <title><data:blog.pageName/> | <data:blog.title/></title>
</b:if>
<b:comment>=== Search (label) page ===</b:comment>
<b:if cond='data:view.search.label'>
  <title><data:blog.pageName/> | <data:blog.title/></title>
</b:if>
<b:comment>=== Search (query) page ===</b:comment>
<b:if cond='data:view.search.query'>
  <title><data:view.search.resultsMessage/> | <data:blog.title/></title>
</b:if>
<b:comment>=== Search (default) page ===</b:comment>
<b:if cond='data:view.search and !data:view.search.label and !data:view.search.query'>
  <title><data:blog.title/></title>
</b:if>
<b:comment>=== Archive page ===</b:comment>
<b:if cond='data:view.isArchive'>
  <title><data:view.archive.rangeMessage/> | <data:blog.title/></title>
</b:if>
<b:comment>=== Error page ===</b:comment>
<b:if cond='data:view.isError'>
  <title>404 | <data:blog.title/></title>
</b:if>

<b:comment>### Custom all-head-content ###</b:comment>

<meta content='blogger' name='generator'/>
<link expr:href='data:blog.blogspotFaviconUrl' rel='icon' type='image/x-icon'/>
<data:blog.feedLinks/>
<data:blog.openIdOpTag/>
<data:blog.meTag/>
<b:if cond='data:blog.googleProfileUrl'>
  <link expr:href='data:blog.googleProfileUrl' rel='publisher'/>
</b:if>
<b:if cond='data:blog.adultContent'>
  <meta content='adult' name='rating'/>
</b:if>
<link expr:href='data:view.url.canonical' rel='canonical'/>
<b:comment>=== Homepage ===</b:comment>
<b:if cond='data:view.isHomepage'>
  <b:if cond='data:blog.metaDescription'>
    <meta expr:content='data:blog.metaDescription' name='description'/>
  <b:else/>
    <meta expr:content='data:blog.title' name='description'/>
  </b:if>
  <link expr:href='data:social-logo' rel='image_src'/>
</b:if>
<b:comment>=== Item page ===</b:comment>
<b:if cond='data:view.isPost'>
  <b:if cond='data:blog.metaDescription'>
    <meta expr:content='data:blog.metaDescription' name='description'/>
  <b:else/>
    <meta expr:content='data:blog.pageName' name='description'/>
  </b:if>
  <b:if cond='data:view.featuredImage'>
    <link expr:href='resizeImage(data:view.featuredImage, 1200, "1200:630")' rel='image_src'/>
  <b:elseif cond='data:blog.postImageUrl'/>
    <link expr:href='resizeImage(data:blog.postImageUrl, 1200, "1200:630")' rel='image_src'/>
  <b:else/>
    <link expr:href='data:social-logo' rel='image_src'/>
  </b:if>
</b:if>
<b:comment>=== Static page ===</b:comment>
<b:if cond='data:view.isPage'>
  <b:if cond='data:blog.metaDescription'>
    <meta expr:content='data:blog.metaDescription' name='description'/>
  <b:else/>
    <meta expr:content='data:blog.pageName' name='description'/>
  </b:if>
  <b:if cond='data:view.featuredImage'>
    <link expr:href='resizeImage(data:view.featuredImage, 1200, "1200:630")' rel='image_src'/>
  <b:elseif cond='data:blog.postImageUrl'/>
    <link expr:href='resizeImage(data:blog.postImageUrl, 1200, "1200:630")' rel='image_src'/>
  <b:else/>
    <link expr:href='data:social-logo' rel='image_src'/>
  </b:if>
</b:if>
<b:comment>=== Search (label) page ===</b:comment>
<b:if cond='data:view.search.label'>
  <meta expr:content='data:view.search.resultsMessage' name='description'/>
  <link expr:href='"https://placehold.it/1200x630/777/eee?text=" + data:blog.pageName' rel='image_src'/>
</b:if>
<b:comment>=== Search (query) page ===</b:comment>
<b:if cond='data:view.search.query'>
  <meta expr:content='data:view.search.resultsMessage' name='description'/>
  <link href='https://placehold.it/1200x630/777/eee?text=Search+Results' rel='image_src'/>
</b:if>
<b:comment>=== Search (default) page ===</b:comment>
<b:if cond='data:view.search and !data:view.search.label and !data:view.search.query'>
  <b:if cond='data:blog.metaDescription'>
    <meta expr:content='data:blog.metaDescription' name='description'/>
  <b:else/>
    <meta expr:content='data:blog.title' name='description'/>
  </b:if>
  <link expr:href='data:social-logo' rel='image_src'/>
</b:if>
<b:comment>=== Archive page ===</b:comment>
<b:if cond='data:view.isArchive'>
  <meta expr:content='data:view.archive.rangeMessage' name='description'/>
  <link expr:href='"https://placehold.it/1200x630/777/eee?text=" + data:view.archive.year' rel='image_src'/>
</b:if>
<b:comment>=== Error page ===</b:comment>
<b:if cond='data:view.isError'>
  <meta content='404' name='description'/>
  <link href='https://placehold.it/1200x630/777/eee?text=404' rel='image_src'/>
</b:if>

<b:comment>### Open Graph ###</b:comment>

<b:if cond='data:view.type == "item"'>
  <meta content='article' property='og:type'/>
<b:else/>
  <meta content='website' property='og:type'/>
</b:if>
<meta expr:content='data:blog.title' property='og:site_name'/>
<meta expr:content='data:facebook-app-id' property='fb:app_id'/>
<meta expr:content='data:view.url.canonical' property='og:url'/>
<meta content='1200' property='og:image:width'/>
<meta content='630' property='og:image:height'/>
<b:comment>=== Homepage ===</b:comment>
<b:if cond='data:view.isHomepage'>
  <meta expr:content='data:blog.title' property='og:title'/>
  <b:if cond='data:blog.metaDescription'>
    <meta expr:content='data:blog.metaDescription' property='og:description'/>
  <b:else/>
    <meta expr:content='data:blog.title' property='og:description'/>
  </b:if>
  <meta expr:content='data:social-logo' property='og:image'/>
</b:if>
<b:comment>=== Item page ===</b:comment>
<b:if cond='data:view.isPost'>
  <meta expr:content='data:blog.pageName + " | " + data:blog.title' property='og:title'/>
  <b:if cond='data:blog.metaDescription'>
    <meta expr:content='data:blog.metaDescription' property='og:description'/>
  <b:else/>
    <meta expr:content='data:blog.pageName' property='og:description'/>
  </b:if>
  <b:if cond='data:view.featuredImage'>
    <meta expr:content='resizeImage(data:view.featuredImage, 1200, "1200:630")' property='og:image'/>
  <b:elseif cond='data:blog.postImageUrl'/>
    <meta expr:content='resizeImage(data:blog.postImageUrl, 1200, "1200:630")' property='og:image'/>
  <b:else/>
    <meta expr:content='data:social-logo' property='og:image'/>
  </b:if>
</b:if>
<b:comment>=== Static page ===</b:comment>
<b:if cond='data:view.isPage'>
  <meta expr:content='data:blog.pageName + " | " + data:blog.title' property='og:title'/>
  <b:if cond='data:blog.metaDescription'>
    <meta expr:content='data:blog.metaDescription' property='og:description'/>
  <b:else/>
    <meta expr:content='data:blog.pageName' property='og:description'/>
  </b:if>
  <b:if cond='data:view.featuredImage'>
    <meta expr:content='resizeImage(data:view.featuredImage, 1200, "1200:630")' property='og:image'/>
  <b:elseif cond='data:blog.postImageUrl'/>
    <meta expr:content='resizeImage(data:blog.postImageUrl, 1200, "1200:630")' property='og:image'/>
  <b:else/>
    <meta expr:content='data:social-logo' property='og:image'/>
  </b:if>
</b:if>
<b:comment>=== Search (label) page ===</b:comment>
<b:if cond='data:view.search.label'>
  <meta expr:content='data:blog.pageName + " | " + data:blog.title' property='og:title'/>
  <meta expr:content='data:view.search.resultsMessage' property='og:description'/>
  <meta expr:content='"https://placehold.it/1200x630/777/eee?text=" + data:blog.pageName' property='og:image'/>
</b:if>
<b:comment>=== Search (query) page ===</b:comment>
<b:if cond='data:view.search.query'>
  <meta expr:content='data:view.search.resultsMessage + " | " + data:blog.title' property='og:title'/>
  <meta expr:content='data:view.search.resultsMessage' property='og:description'/>
  <meta content='https://placehold.it/1200x630/777/eee?text=Search+Results' property='og:image'/>
</b:if>
<b:comment>=== Search (default) page ===</b:comment>
<b:if cond='data:view.search and !data:view.search.label and !data:view.search.query'>
  <meta expr:content='data:blog.title' property='og:title'/>
  <b:if cond='data:blog.metaDescription'>
    <meta expr:content='data:blog.metaDescription' property='og:description'/>
  <b:else/>
    <meta expr:content='data:blog.title' property='og:description'/>
  </b:if>
  <meta expr:content='data:social-logo' property='og:image'/>
</b:if>
<b:comment>=== Archive page ===</b:comment>
<b:if cond='data:view.isArchive'>
  <meta expr:content='data:view.archive.rangeMessage + " | " + data:blog.title' property='og:title'/>
  <meta expr:content='data:view.archive.rangeMessage' property='og:description'/>
  <meta expr:content='"https://placehold.it/1200x630/777/eee?text=" + data:view.archive.year' property='og:image'/>
</b:if>
<b:comment>=== Error page ===</b:comment>
<b:if cond='data:view.isError'>
  <meta expr:content='"404 | " + data:blog.title' property='og:title'/>
  <meta content='404' property='og:description'/>
  <meta content='https://placehold.it/1200x630/777/eee?text=404' property='og:image'/>
</b:if>

<b:comment>### Twitter Cards ###</b:comment>

<b:if cond='data:view.isHomepage'>
  <meta content='summary_large_image' name='twitter:card'/>
<b:else/>
  <meta content='summary' name='twitter:card'/>
</b:if>
<meta expr:content='data:twitter-username-website' name='twitter:site'/>
